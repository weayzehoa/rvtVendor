APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost
APP_TIMEZONE=Asia/Taipei
APP_LOCALE=zh_TW

WEB_DOMAIN=localhost
WEB_API_DOMAIN=webapi.localhost
ADMIN_API_DOMAIN=admapi.localhost

LOG_CHANNEL=stack
LOG_LEVEL=debug

DB_CONNECTION=mysql
