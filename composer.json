{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "alibabacloud/darabonba-openapi": "^0.1.8",
        "alibabacloud/dysmsapi-20170525": "1.0.2",
        "arcanedev/no-captcha": "^12.1",
        "aws/aws-sdk-php-laravel": "^3.9",
        "barryvdh/laravel-dompdf": "^2.0",
        "danielme85/laravel-log-to-db": "^3.0",
        "darkaonline/l5-swagger": "^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "ixudra/curl": "^6.21",
        "laravel-lang/lang": "~7.0",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-sftp": "~1.0",
        "littlebookboy/laravel-request-recorder": "^1.3",
        "maatwebsite/excel": "^3.1",
        "milon/barcode": "^8.0",
        "mingjshk/laravel-newebpay": "^1.0",
        "pragmarx/google2fa": "^8.0",
        "predis/predis": "^1.1",
        "proengsoft/laravel-jsvalidation": "^4.4",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/image": "^1.10",
        "spatie/laravel-activitylog": "^3.16",
        "spatie/laravel-image-optimizer": "^1.6",
        "tinify/tinify": "^1.5",
        "twilio/sdk": "^6.28",
        "tymon/jwt-auth": "^1.0",
        "vanry/laravel-scout-tntsearch": "^2.3",
        "webpatser/laravel-uuid": "^4.0",
        "zanysoft/laravel-zip": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^0.0.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
